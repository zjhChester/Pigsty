
.image-box {
		
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-orient: vertical;
		-webkit-box-direction: normal;
		-webkit-flex-direction: column;
		        flex-direction: column;
		
		height: 350rpx;
		overflow: hidden;
}
.image {
		
		width: 100%;
		height: 100%;
		
		-webkit-box-flex: 1;
		-webkit-flex: 1;
		        flex: 1;
}
.content-box {
		padding-top: 20rpx;
}
.content-box-text {
		font-size: 30rpx;
}
